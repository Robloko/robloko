local function debugAllAPIs()
    print("=== DEBUGGING ALL APIs ===")
    
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    
    if not ReplicatedStorage:FindFirstChild("API") then
        print("âŒ API folder not found in ReplicatedStorage")
        return
    end
    
    print("ðŸ“ API folder found. Listing all endpoints:")
    
    local apiCount = 0
    for _, api in ipairs(ReplicatedStorage.API:GetChildren()) do
        apiCount = apiCount + 1
        print(apiCount .. ". " .. api.Name)
    end
    
    print("ðŸ“Š Total APIs found: " .. apiCount)
    
    -- Specifically look for MonitorAPI and AilmentsAPI
    print("\nðŸ” Looking for specific APIs:")
    
    local monitorAPI = ReplicatedStorage.API:FindFirstChild("MonitorAPI/AddAdditive")
    if monitorAPI then
        print("âœ… FOUND: MonitorAPI/AddAdditive")
    else
        print("âŒ NOT FOUND: MonitorAPI/AddAdditive")
    end
    
    local ailmentsAPI = ReplicatedStorage.API:FindFirstChild("AilmentsAPI/BabyAilmentCompleted")
    if ailmentsAPI then
        print("âœ… FOUND: AilmentsAPI/BabyAilmentCompleted")
    else
        print("âŒ NOT FOUND: AilmentsAPI/BabyAilmentCompleted")
    end
    
    -- Look for any API containing "Monitor" or "Ailment" or "Baby"
    print("\nðŸ”Ž Searching for related APIs:")
    
    local relatedAPIs = {}
    for _, api in ipairs(ReplicatedStorage.API:GetChildren()) do
        local apiName = api.Name:lower()
        if apiName:find("monitor") or apiName:find("ailment") or apiName:find("baby") or apiName:find("add") then
            table.insert(relatedAPIs, api.Name)
        end
    end
    
    if #relatedAPIs > 0 then
        print("Related APIs found:")
        for i, apiName in ipairs(relatedAPIs) do
            print("  " .. i .. ". " .. apiName)
        end
    else
        print("No related APIs found")
    end
    
    print("=== DEBUG COMPLETE ===")
end

-- Run the debug function
debugAllAPIs()
