local function debugBabyData()
    print("=== DEBUGGING BABY DATA ===")
    
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local Players = game:GetService("Players")
    local localPlayer = Players.LocalPlayer
    
    -- Get player data
    local clientDataModule = ReplicatedStorage:FindFirstChild("ClientModules")
    local coreModule = clientDataModule and clientDataModule:FindFirstChild("Core")
    local clientDataScript = coreModule and coreModule:FindFirstChild("ClientData")
    
    if not clientDataScript then
        print("‚ùå Cannot access ClientData")
        return
    end
    
    local clientData = require(clientDataScript)
    local playerData = clientData.get_data()[localPlayer.Name]
    
    if not playerData then
        print("‚ùå Cannot get player data")
        return
    end
    
    -- Debug baby data
    if playerData.babies then
        print("üìä BABIES DATA:")
        for babyId, babyData in pairs(playerData.babies) do
            print("Baby ID:", babyId)
            print("  Data:", babyData)
            
            -- Check if baby has specific properties
            if type(babyData) == "table" then
                for key, value in pairs(babyData) do
                    print("  " .. key .. ":", value)
                end
            end
        end
    else
        print("‚ùå No babies data found")
    end
    
    -- Debug ailments data more thoroughly
    if playerData.ailments_manager and playerData.ailments_manager.baby_ailments then
        print("\nüìä BABY AILMENTS DETAILS:")
        for ailmentType, ailmentData in pairs(playerData.ailments_manager.baby_ailments) do
            print("Ailment:", ailmentType)
            print("  Full data:", ailmentData)
            
            if type(ailmentData) == "table" then
                for key, value in pairs(ailmentData) do
                    print("  " .. key .. ":", value)
                    
                    -- Check if there's a baby reference in the ailment data
                    if type(value) == "table" then
                        print("    Nested data:")
                        for k2, v2 in pairs(value) do
                            print("    " .. k2 .. ":", v2)
                        end
                    end
                end
            end
        end
    end
    
    print("=== DEBUG COMPLETE ===")
end

-- Run the debug
debugBabyData()
