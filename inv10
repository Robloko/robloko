-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Player
local player = Players.LocalPlayer

-- Simple function to safely inspect house manager
local function simpleInspectHouseManager()
    print("ğŸ  SIMPLE HOUSE MANAGER INSPECTION")
    print("==================================")
    
    -- Safely get data
    local success, clientData = pcall(require, ReplicatedStorage.ClientModules.Core.ClientData)
    if not success then
        print("âŒ Failed to require ClientData")
        return
    end
    
    local playerData = clientData.get_data()
    if not playerData then
        print("âŒ No player data from get_data()")
        return
    end
    
    local myData = playerData[player.Name]
    if not myData then
        print("âŒ No data for player: " .. player.Name)
        return
    end
    
    print("âœ… Player data found!")
    
    -- Check if house_manager exists
    if myData.house_manager then
        print("âœ… house_manager found!")
        local hm = myData.house_manager
        
        -- Simple count of keys
        local keyCount = 0
        for _ in pairs(hm) do keyCount = keyCount + 1 end
        print("ğŸ“Š Total keys in house_manager: " .. keyCount)
        print("")
        
        -- List all keys with basic info
        for key, value in pairs(hm) do
            if type(value) == "table" then
                local tableCount = 0
                for _ in pairs(value) do tableCount = tableCount + 1 end
                print("ğŸ“ " .. key .. " [Table with " .. tableCount .. " items]")
            else
                print("ğŸ“„ " .. key .. " = " .. tostring(value))
            end
        end
        
    else
        print("âŒ house_manager not found")
        print("Available keys in player data:")
        for key, value in pairs(myData) do
            if type(value) == "table" then
                print("ğŸ“ " .. key)
            else
                print("ğŸ“„ " .. key .. " = " .. tostring(value))
            end
        end
    end
end

-- Run the simple inspection
simpleInspectHouseManager()
