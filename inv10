-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Player
local player = Players.LocalPlayer

-- Function for ultra-deep inspection
local function ultraDeepInspection()
    print("üîç ULTRA DEEP HOUSE MANAGER INSPECTION")
    print("======================================")
    
    local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
    local myData = clientData.get_data()[player.Name]
    local hm = myData.house_manager
    
    -- Find and inspect the main table
    for mainKey, mainTable in pairs(hm) do
        if type(mainTable) == "table" then
            print("üè† MAIN TABLE: " .. mainKey)
            print("Contains " .. countKeys(mainTable) .. " items")
            print("")
            
            -- Inspect each item in the main table
            for itemKey, itemValue in pairs(mainTable) do
                print("üì¶ ITEM: " .. tostring(itemKey))
                
                if type(itemValue) == "table" then
                    print("   Type: Table with " .. countKeys(itemValue) .. " properties")
                    
                    -- Show all properties of this item
                    for propKey, propValue in pairs(itemValue) do
                        if type(propValue) == "table" then
                            local subCount = countKeys(propValue)
                            print("   ‚îú‚îÄ " .. propKey .. " = [Table with " .. subCount .. " items]")
                        else
                            print("   ‚îú‚îÄ " .. propKey .. " = " .. tostring(propValue))
                        end
                    end
                else
                    print("   Type: " .. type(itemValue))
                    print("   Value: " .. tostring(itemValue))
                end
                print("   " .. string.rep("-", 30))
            end
        end
    end
end

-- Helper function
local function countKeys(tbl)
    if type(tbl) ~= "table" then return 0 end
    local count = 0
    for _ in pairs(tbl) do count = count + 1 end
    return count
end

-- Run the ultra deep inspection
ultraDeepInspection()
