-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Player
local player = Players.LocalPlayer

-- Function to inspect house manager tables in detail
local function inspectHouseManagerTables()
    print("üîç HOUSE MANAGER TABLES INSPECTION")
    print("==================================")
    
    -- Get data
    local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
    local myData = clientData.get_data()[player.Name]
    
    if not myData.house_manager then
        print("‚ùå No house_manager")
        return
    end
    
    local hm = myData.house_manager
    
    -- Inspect each table in house_manager
    for key, value in pairs(hm) do
        if type(value) == "table" then
            print("")
            print("üìÇ " .. key .. ":")
            
            local itemCount = 0
            for _ in pairs(value) do itemCount = itemCount + 1 end
            print("   Total items: " .. itemCount)
            
            -- Show first 10 items
            local shown = 0
            for subKey, subValue in pairs(value) do
                if shown < 10 then
                    if type(subValue) == "table" then
                        print("   ‚îú‚îÄ " .. tostring(subKey) .. " = [TABLE]")
                    else
                        print("   ‚îú‚îÄ " .. tostring(subKey) .. " = " .. tostring(subValue))
                    end
                    shown = shown + 1
                else
                    print("   ‚îî‚îÄ ... and " .. (itemCount - 10) .. " more items")
                    break
                end
            end
        end
    end
end

-- Run both inspections
simpleInspectHouseManager()
print("\n" .. string.rep("=", 50) .. "\n")
inspectHouseManagerTables()
