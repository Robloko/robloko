
-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Player
local player = Players.LocalPlayer

-- Function to inspect specific categories from screenshots
local function inspectScreenshotCategories()
    print("üîç INSPECTING CATEGORIES FROM SCREENSHOTS")
    print("=========================================")
    
    local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
    local data = clientData.get_data()[player.Name]
       
    -- Inspect playtime_payouts_manager
    if data.playtime_payouts_manager then
        print("‚è∞ PLAYTIME PAYOUTS MANAGER:")
        for key, value in pairs(data.playtime_payouts_manager) do
            if type(value) == "table" then
                print("   üìÅ " .. key .. " [TABLE]")
                for subKey, subValue in pairs(value) do
                    print("     " .. subKey .. " = " .. tostring(subValue))
                end
            else
                print("   üìÑ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    end

-- Run the function
inspectScreenshotCategories()
