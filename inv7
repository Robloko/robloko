-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Player
local player = Players.LocalPlayer

-- Function to inspect specific categories from screenshots
local function inspectScreenshotCategories()
    print("ğŸ” INSPECTING CATEGORIES")
    print("=========================================")
    
    local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
    local data = clientData.get_data()[player.Name]
    
    -- Inspect house_manager
    if data.house_manager then
        print("AILMENTS MANAGER:")
        for key, value in pairs(data.ailments_manager) do
            if type(value) == "table" then
                local count = 0
                for _ in pairs(value) do count = count + 1 end
                print("   ğŸ“ " .. key .. " [TABLE with " .. count .. " items]")
            else
                print("   ğŸ“„ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    end
    
    -- Inspect pet_state_managers_raw
    if data.pet_state_managers_raw then
        print("daily_login_manager:")
        for petId, petState in pairs(data.daily_login_manager) do
            print("   Pet ID: " .. tostring(petId))
            for stateKey, stateValue in pairs(petState) do
                if type(stateValue) == "table" then
                    print("     ğŸ“ " .. stateKey .. " [TABLE]")
                else
                    print("     ğŸ“„ " .. stateKey .. " = " .. tostring(stateValue))
                end
            end
            print("   ---")
        end
        print("")
    end
    
    -- Inspect playtime_payouts_manager
    if data.playtime_payouts_manager then
        print("â° PLAYTIME PAYOUTS MANAGER:")
        for key, value in pairs(data.playtime_payouts_manager) do
            if type(value) == "table" then
                print("   ğŸ“ " .. key .. " [TABLE]")
                for subKey, subValue in pairs(value) do
                    print("     " .. subKey .. " = " .. tostring(subValue))
                end
            else
                print("   ğŸ“„ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    end
    
    -- Inspect pet_recycler_manager
    if data.pet_recycler_manager then
        print("â™»ï¸ PET RECYCLER MANAGER:")
        for key, value in pairs(data.pet_recycler_manager) do
            if type(value) == "table" then
                local count = 0
                for _ in pairs(value) do count = count + 1 end
                print("   ğŸ“ " .. key .. " [TABLE with " .. count .. " items]")
            else
                print("   ğŸ“„ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    end
end

-- Run the function
inspectScreenshotCategories()
