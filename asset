local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Function to find and test image assets
local function findAndDisplayImages()
    local foundAssets = {}
    
    -- Search in different places for images
    local placesToSearch = {
        game:GetService("Textures"),
        game:GetService("Lighting"),
        game:GetService("StarterPack"),
        game:GetService("StarterGui"),
        game:GetService("ReplicatedStorage"),
        game:GetService("ServerStorage"),
        game:GetService("Workspace")
    }
    
    -- Common Roblox image asset IDs that usually work
    local commonImageIds = {
        13300463778,  -- Gift icon
        10734951899,  -- Star icon
        10734952199,  -- Heart icon
        10734952499,  -- Diamond icon
        10734952799,  -- Club icon
        10734953099,  -- Spade icon
        10734953399,  -- Circle icon
        10734953699,  -- Square icon
        10734953999,  -- Triangle icon
        10734954299,  -- Cross icon
        10734954599,  -- Check icon
        10734954899,  -- Question icon
        10734955199,  -- Exclamation icon
        10734955499,  -- Plus icon
        10734955799,  -- Minus icon
    }
    
    -- Test common images first
    for _, assetId in ipairs(commonImageIds) do
        local testImage = Instance.new("ImageLabel")
        testImage.Size = UDim2.new(0, 50, 0, 50)
        testImage.Position = UDim2.new(0, (#foundAssets * 60) + 20, 0, 20)
        testImage.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        testImage.Image = "rbxassetid://" .. assetId
        testImage.Parent = screenGui
        
        -- Add ID label
        local idLabel = Instance.new("TextLabel")
        idLabel.Size = UDim2.new(1, 0, 0, 15)
        idLabel.Position = UDim2.new(0, 0, 1, 2)
        idLabel.BackgroundTransparency = 1
        idLabel.Text = assetId
        idLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        idLabel.TextSize = 10
        idLabel.Parent = testImage
        
        table.insert(foundAssets, {id = assetId, object = testImage})
    end
    
    print("Testing " .. #commonImageIds .. " common image assets...")
    
    -- Check which images loaded successfully
    task.wait(2) -- Wait for images to load
    
    local workingCount = 0
    for i, asset in ipairs(foundAssets) do
        if asset.object.Image ~= "" then
            workingCount = workingCount + 1
            print("Working image: " .. asset.id)
        else
            asset.object:Destroy() -- Remove broken images
        end
    end
    
    print("Found " .. workingCount .. " working images!")
    
    -- Display the first working image in center
    for i, asset in ipairs(foundAssets) do
        if asset.object and asset.object.Parent then
            -- Move first working image to center
            asset.object.Size = UDim2.new(0, 100, 0, 100)
            asset.object.Position = UDim2.new(0.5, -50, 0.5, -50)
            asset.object.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
            
            -- Update label
            local label = asset.object:FindFirstChildOfClass("TextLabel")
            if label then
                label.Text = "Working: " .. asset.id
                label.TextSize = 12
            end
            
            print("Main display using asset ID: " .. asset.id)
            break
        end
    end
end

-- Run the search
findAndDisplayImages()
