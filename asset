-- Fresh script - no functions, no errors
print("=== STARTING IMAGE SCAN ===")

local foundCount = 0

-- Check workspace
print("Checking workspace...")
for _, obj in pairs(workspace:GetDescendants()) do
    if obj:IsA("ImageLabel") and obj.Image ~= "" then
        print("FOUND: " .. obj.Image)
        foundCount = foundCount + 1
    end
end

-- Check Lighting
print("Checking Lighting...")
for _, obj in pairs(game.Lighting:GetDescendants()) do
    if obj:IsA("ImageLabel") and obj.Image ~= "" then
        print("FOUND: " .. obj.Image)
        foundCount = foundCount + 1
    end
end

-- Check StarterGui
print("Checking StarterGui...")
for _, obj in pairs(game.StarterGui:GetDescendants()) do
    if obj:IsA("ImageLabel") and obj.Image ~= "" then
        print("FOUND: " .. obj.Image)
        foundCount = foundCount + 1
    end
end

print("=== SCAN FINISHED ===")
print("Total images found: " .. foundCount)
