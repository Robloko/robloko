local function scanGameForImages()
    print("=== SCANNING FOR IMAGES AND TEXTURES ===")
    
    local placesToCheck = {
        workspace,
        game:GetService("Lighting"),
        game:GetService("StarterPack"),
        game:GetService("StarterGui"),
        game:GetService("ReplicatedStorage"),
        game:GetService("ServerStorage"),
    }
    
    local foundImages = {}
    
    for _, place in ipairs(placesToCheck) do
        print("\nScanning: " .. place:GetFullName())
        
        local descendants = place:GetDescendants()
        for _, obj in ipairs(descendants) do
            if obj:IsA("ImageLabel") and obj.Image ~= "" then
                table.insert(foundImages, obj.Image)
                print("ImageLabel: " .. obj.Image)
            elseif obj:IsA("ImageButton") and obj.Image ~= "" then
                table.insert(foundImages, obj.Image)
                print("ImageButton: " .. obj.Image)
            elseif obj:IsA("Decal") and obj.Texture ~= "" then
                table.insert(foundImages, obj.Texture)
                print("Decal: " .. obj.Texture)
            elseif obj:IsA("Texture") and obj.Texture ~= "" then
                table.insert(foundImages, obj.Texture)
                print("Texture: " .. obj.Texture)
            end
        end
    end
    
    print("\n=== SCAN COMPLETE ===")
    print("Found " .. #foundImages .. " images/textures total")
    
    return foundImages
end

-- Run the scan
scanGameForImages()
