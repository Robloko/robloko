-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Player
local player = Players.LocalPlayer

-- Function to inspect news_app_details
local function inspectNewsAppDetails()
    print("ğŸ” INSPECTING NEWS_APP_DETAILS")
    print("==============================")
    
    local success, data = pcall(function()
        local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
        return clientData.get_data()[player.Name]
    end)
    
    if not success or not data then
        print("âŒ Failed to get player data")
        return
    end
    
    -- Check if news_app_details exists
    if data.news_app_details then
        print("âœ… Found news_app_details!")
        print("Type: " .. type(data.news_app_details))
        
        -- Print all contents
        for key, value in pairs(data.news_app_details) do
            if type(value) == "table" then
                print("ğŸ“ " .. key .. ": [TABLE]")
                for subKey, subValue in pairs(value) do
                    print("   " .. subKey .. " = " .. tostring(subValue))
                end
            else
                print("ğŸ“„ " .. key .. " = " .. tostring(value))
            end
        end
    else
        print("âŒ news_app_details not found in player data")
        
        -- Show what data sections are available
        print("ğŸ“‹ Available data sections:")
        for key, value in pairs(data) do
            print("   " .. key .. " (" .. type(value) .. ")")
        end
    end
end

-- Run the function
inspectNewsAppDetails()
