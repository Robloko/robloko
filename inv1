-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Player
local player = Players.LocalPlayer

-- Simple function to inspect all categories
local function inspectAllCategories()
    print("ğŸ” INSPECTING ALL CATEGORIES")
    print("============================")
    
    -- First test if we can get the module
    local success1, clientData = pcall(function()
        return require(ReplicatedStorage.ClientModules.Core.ClientData)
    end)
    
    if not success1 then
        print("âŒ Failed to load ClientData: " .. tostring(clientData))
        return
    end
    
    print("âœ… ClientData module loaded")
    
    -- Test if we can call get_data
    local success2, allData = pcall(function()
        return clientData.get_data()
    end)
    
    if not success2 then
        print("âŒ Failed to call get_data: " .. tostring(allData))
        return
    end
    
    print("âœ… get_data() works")
    
    -- Test if we can get player data
    if not allData[player.Name] then
        print("âŒ No data found for player: " .. player.Name)
        return
    end
    
    local data = allData[player.Name]
    print("âœ… Found player data")
    
    -- Now safely list all categories
    print("ğŸ“‹ ALL CATEGORIES:")
    for categoryName, categoryData in pairs(data) do
        print("ğŸ¯ " .. categoryName .. " (" .. type(categoryData) .. ")")
    end
end

-- Run the function
inspectAllCategories()
