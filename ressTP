--------------------------------------------------------------
-- VARIABLES & CONFIG
--------------------------------------------------------------
local running = false
local farmThread = nil
local petRecyclerModel = nil -- Store the found recycler model

-- RELEASE CONDITIONS (SMART PRIORITY SYSTEM)
local RELEASE_CONDITIONS = {
    MIN_PETS = 18, -- Condition 1: 18 allowed basic pets
    MIN_NEONS = 2, -- Condition 2: 2 neon pets (any type, age 6+)
    MIN_MEGA_NEONS = 1, -- Condition 3: 1 mega neon pet (any age)
}

-- COOLDOWN TIMERS
local COOLDOWNS = {
    REWARD_WAIT = 425, -- 7 minutes 5 seconds (425 seconds)
    AFTER_CLAIM = 30, -- 30 seconds after claim
    RESOURCE_CHECK = 60 -- 60 seconds between condition checks
}

-- STRICTLY ALLOWED PET TYPES
local ALLOWED_PET_NAMES = {
    "basic_egg_2022_camel",
    "basic_egg_2022_poodle",
    "basic_egg_2022_donkey",
    "basic_egg_2022_zebra",
    "basic_egg_2022_orangutan",
    "basic_egg_2022_parakeet",
    "basic_egg_2022_swordfish",
    "basic_egg_2022_robot",
    "basic_egg_2022_corgi"
}

--------------------------------------------------------------
-- DYNAMIC PET RECYCLER FINDER (RETURNS THE MODEL)
--------------------------------------------------------------
local function log(msg) 
    local timestamp = os.date("%H:%M:%S")
    print("[" .. timestamp .. "] [Nursery Farmer] " .. tostring(msg)) 
end

local function findPetRecyclerModel()
    local Workspace = game:GetService("Workspace")
    
    -- First check in MainMap!EggTeaser2026 (event area)
    local eggTeaser = Workspace:FindFirstChild("MainMap!EggTeaser2026")
    if eggTeaser then
        log("üîç Searching in MainMap!EggTeaser2026...")
        -- Search recursively in EggTeaser
        local function searchInEggTeaser(folder)
            for _, child in ipairs(folder:GetChildren()) do
                if child.Name == "PetRecycler" then
                    return child, child:GetFullName()
                end
                if child:IsA("Folder") or child:IsA("Model") then
                    local found, path = searchInEggTeaser(child)
                    if found then
                        return found, path
                    end
                end
            end
            return nil
        end
        
        local recycler, path = searchInEggTeaser(eggTeaser)
        if recycler then
            log("‚úÖ Found PetRecycler in MainMap!EggTeaser2026!")
            log("üìã Full path: " .. path)
            return recycler
        end
    end
    
    -- If not found in EggTeaser, search entire workspace
    log("üîç Searching entire workspace for PetRecycler...")
    local function searchWorkspace(folder)
        for _, child in ipairs(folder:GetChildren()) do
            if child.Name == "PetRecycler" then
                return child
            end
            if child:IsA("Folder") or child:IsA("Model") then
                local found = searchWorkspace(child)
                if found then
                    return found
                end
            end
        end
        return nil
    end
    
    return searchWorkspace(Workspace)
end

--------------------------------------------------------------
-- GET RECYCLER PARENT FOLDER FOR SLOT NAME
--------------------------------------------------------------
local function getRecyclerParentFolder()
    if not petRecyclerModel then
        petRecyclerModel = findPetRecyclerModel()
    end
    
    if not petRecyclerModel then
        log("‚ö†Ô∏è PetRecycler not found in workspace")
        return nil
    end
    
    -- Get the parent folder (should be the slot folder like f-0, f-1, etc.)
    local parent = petRecyclerModel.Parent
    
    -- Check if parent name matches f- pattern
    if parent and string.match(parent.Name, "^f%-%d+$") then
        log("‚úÖ Found recycler in folder: " .. parent.Name)
        return parent.Name
    else
        -- If direct parent isn't f- folder, try to find the housing folder
        local current = petRecyclerModel
        for i = 1, 5 do -- Check up to 5 levels up
            current = current.Parent
            if not current then break end
            if string.match(current.Name, "^f%-%d+$") then
                log("‚úÖ Found recycler in folder (nested): " .. current.Name)
                return current.Name
            end
        end
        
        -- For main map recyclers, they might not be in f- folders
        log("‚ö†Ô∏è Recycler is not in an f- folder (main map/event recycler)")
        -- Return the actual model path for main map recyclers
        return nil -- We'll handle this case separately
    end
end

--------------------------------------------------------------
-- DIRECT INTERACTION WITH FOUND RECYCLER
--------------------------------------------------------------
local function interactWithRecycler(action, uniques)
    if not petRecyclerModel then
        petRecyclerModel = findPetRecyclerModel()
    end
    
    if not petRecyclerModel then
        log("‚ùå Cannot interact: PetRecycler not found")
        return false
    end
    
    log("üéØ Attempting direct interaction with PetRecycler at: " .. petRecyclerModel:GetFullName())
    
    -- Try different interaction methods
    local success = false
    
    -- Method 1: Try ClickDetector if available
    local clickDetector = petRecyclerModel:FindFirstChild("ClickDetector")
    if clickDetector then
        log("üñ±Ô∏è Using ClickDetector")
        pcall(function()
            fireclickdetector(clickDetector)
        end)
        task.wait(1)
        -- After clicking, we might need to send the API request
    end
    
    -- Method 2: Try to find the parent slot and use HousingAPI
    local parentFolder = getRecyclerParentFolder()
    if parentFolder then
        log("üè† Using HousingAPI with folder: " .. parentFolder)
        local args = {
            parentFolder,
            "UseBlock",
            action == "use" and { action = "use", uniques = uniques } or { action = "claim" },
            player.Character
        }
        
        success = pcall(function()
            local result = ReplicatedStorage:WaitForChild("API"):WaitForChild("HousingAPI/ActivateInteriorFurniture"):InvokeServer(unpack(args))
            return result ~= nil
        end)
    end
    
    -- Method 3: For main map recyclers, try direct RemoteEvent
    if not success then
        log("üì° Trying direct RemoteEvent interaction...")
        
        -- Try different possible remote events
        local remotePaths = {
            "PetRecyclerAPI/RecyclePets",
            "PetRecyclerAPI/Activate",
            "HousingAPI/ActivateFurniture",
            "RecycleAPI/Recycle"
        }
        
        for _, remotePath in ipairs(remotePaths) do
            local remote = ReplicatedStorage:FindFirstChild(remotePath)
            if remote then
                log(" Attempting remote: " .. remotePath)
                if action == "use" then
                    success = pcall(function()
                        remote:InvokeServer({ uniques = uniques })
                    end)
                else
                    success = pcall(function()
                        remote:InvokeServer({ action = "claim" })
                    end)
                end
                if success then break end
            end
        end
    end
    
    return success
end

--------------------------------------------------------------
-- PET MANAGEMENT SYSTEM (STRICT FILTERING)
--------------------------------------------------------------
-- üîç Get ALL pets from inventory
local function getAllPets()
    local playerData = clientData.get_data()[player.Name]
    local pets = {}
    if playerData and playerData.inventory and playerData.inventory.pets then
        for uniqueId, petData in pairs(playerData.inventory.pets) do
            if type(petData) == "table" then
                petData.uniqueId = uniqueId
                petData.unique = petData.unique or uniqueId
                table.insert(pets, petData)
            end
        end
    end
    return pets
end

-- üü¢ Get Neon and Mega Neon pets (ANY type)
local function getNeonPets()
    local pets = getAllPets()
    local neons = {}
    local megaNeons = {}
    for _, pet in ipairs(pets) do
        local isNeon = pet.properties and pet.properties.neon
        local isMegaNeon = pet.properties and pet.properties.mega_neon
        local age = pet.properties and pet.properties.age or 0
        if isMegaNeon then
            table.insert(megaNeons, {
                uniqueId = pet.uniqueId,
                age = age,
                id = pet.id
            })
            log("üåü MEGA NEON Found: " .. pet.id)
        elseif isNeon and age >= 6 then
            table.insert(neons, {
                uniqueId = pet.uniqueId,
                age = age,
                id = pet.id
            })
            log("üåü Neon 6+ Found: " .. pet.id .. " (Age: " .. age .. ")")
        elseif isNeon then
            log("üí° Young Neon: " .. pet.id .. " (Age: " .. age .. ") - Too young, skipping")
        end
    end
    return neons, megaNeons
end

-- üîç Get ONLY allowed target pets (strict filter)
local function getAllowedTargetPets()
    local pets = getAllPets()
    local allowedPets = {}
    local counts = {}
    -- Initialize counts for allowed pets only
    for _, name in ipairs(ALLOWED_PET_NAMES) do
        counts[name] = 0
    end
    -- Collect ONLY allowed pets
    for _, pet in ipairs(pets) do
        for _, allowedName in ipairs(ALLOWED_PET_NAMES) do
            if pet.id == allowedName then
                table.insert(allowedPets, pet.uniqueId)
                counts[allowedName] = (counts[allowedName] or 0) + 1
                break
            end
        end
    end
    log("üìä Allowed Pets Scan: " .. HttpService:JSONEncode(counts))
    log("üì¶ Total Allowed Pets: " .. #allowedPets)
    return allowedPets, counts
end

-- üé´ Get Tickets (pet_recycler_tickets_2026)
local function getTickets()
    local tickets = 0
    pcall(function()
        local playerData = clientData.get_data()[player.Name]
        if playerData and playerData.inventory and playerData.inventory.items then
            for _, item in pairs(playerData.inventory.items) do
                if item.id == "pet_recycler_tickets_2026" then
                    tickets = item.amount or 0
                    break
                end
            end
        end
    end)
    ticketsLabel.Text = "Tickets: " .. tickets
    log("üé´ Tickets: " .. tickets)
    return tickets
end

-- ü•ö Count Crystal Eggs
local function countCrystalEggs()
    local playerData = clientData.get_data()[player.Name]
    local count = 0
    if playerData and playerData.inventory and playerData.inventory.pets then
        for _, petData in pairs(playerData.inventory.pets) do
            if petData.id and string.lower(petData.id) == "pet_recycler_2025_crystal_egg" then
                count = count + 1
            end
        end
    end
    log("ü•ö CRYSTAL EGGS: " .. count)
    return count
end

-- üîÑ Refresh inventory data
local function refreshInventory()
    pcall(function()
        ReplicatedStorage:WaitForChild("API"):WaitForChild("InventoryAPI/Request"):InvokeServer()
    end)
    task.wait(2)
end

-- üö´ Anti-AFK
local function antiAfk()
    pcall(function()
        local args = { [1] = { [1] = "AntiAFK" } }
        ReplicatedStorage:WaitForChild("API"):WaitForChild("MiscAPI/OnLoad"):InvokeServer(unpack(args))
    end)
end

--------------------------------------------------------------
-- SMART RELEASE DECISION SYSTEM
--------------------------------------------------------------
local function shouldReleaseNow()
    local allowedPets, counts = getAllowedTargetPets()
    local neonPets, megaNeonPets = getNeonPets()
    local totalAllowedPets = #allowedPets
    local totalNeons = #neonPets
    local totalMegaNeons = #megaNeonPets
    log("ü§î Release Check:")
    log(" Allowed Pets: " .. totalAllowedPets .. "/" .. RELEASE_CONDITIONS.MIN_PETS)
    log(" Neons (6+): " .. totalNeons .. "/" .. RELEASE_CONDITIONS.MIN_NEONS)
    log(" Mega Neons: " .. totalMegaNeons .. "/" .. RELEASE_CONDITIONS.MIN_MEGA_NEONS)
    
    -- PRIORITY-BASED RELEASE CONDITIONS
    if totalMegaNeons >= RELEASE_CONDITIONS.MIN_MEGA_NEONS then
        log("‚úÖ REASON: Enough mega neons (" .. totalMegaNeons .. ")")
        return true, "mega_neons", allowedPets, neonPets, megaNeonPets
    end
    if totalAllowedPets >= RELEASE_CONDITIONS.MIN_PETS then
        log("‚úÖ REASON: Enough allowed pets (" .. totalAllowedPets .. ")")
        return true, "pets", allowedPets, neonPets, megaNeonPets
    end
    if totalNeons >= RELEASE_CONDITIONS.MIN_NEONS then
        log("‚úÖ REASON: Enough neons (" .. totalNeons .. ")")
        return true, "neons", allowedPets, neonPets, megaNeonPets
    end
    log("‚ùå Not enough resources for release")
    return false, "none", allowedPets, neonPets, megaNeonPets
end

--------------------------------------------------------------
-- PET RELEASE LOGIC (USING FOUND FOLDER)
--------------------------------------------------------------
local function executePetRelease(allowedPets, neonPets, megaNeonPets)
    log("üîπ Starting PET release process...")
    local petIds = {}
    local usedNeons = 0
    local usedMegaNeons = 0

    -- Priority 1: Mega Neons
    for i, megaNeon in ipairs(megaNeonPets) do
        if i <= 1 then
            table.insert(petIds, megaNeon.uniqueId)
            usedMegaNeons = usedMegaNeons + 1
            log("üöÄ Adding MEGA NEON: " .. (megaNeon.id or "Unknown"))
        end
    end

    -- Priority 2: Neons (age 6+)
    for i, neon in ipairs(neonPets) do
        if i <= 2 and usedMegaNeons < 1 then
            table.insert(petIds, neon.uniqueId)
            usedNeons = usedNeons + 1
            log("üöÄ Adding Neon: " .. (neon.id or "Unknown") .. " (Age: " .. (neon.age or 0) .. ")")
        end
    end

    -- Priority 3: Allowed Pets
    local slotsRemaining = 18 - usedNeons - usedMegaNeons
    for i = 1, math.min(slotsRemaining, #allowedPets) do
        table.insert(petIds, allowedPets[i])
    end

    log("üéØ Release Strategy: " ..
        usedMegaNeons .. " mega neons + " ..
        usedNeons .. " neons + " ..
        (#petIds - usedNeons - usedMegaNeons) ..
        " allowed pets = " ..
        #petIds .. " total pets")

    if #petIds < 1 then
        log("‚ùå No pets selected for release")
        return false
    end

    -- Create uniques table for API
    local uniques = {}
    for _, petId in ipairs(petIds) do
        uniques[petId] = true
    end

    -- INTERACT WITH THE FOUND RECYCLER DIRECTLY
    local releaseSuccess = interactWithRecycler("use", uniques)

    if releaseSuccess then
        log("‚úÖ Successfully interacted with PetRecycler for release")
        
        -- Collect tickets
        task.wait(1)
        local ticketSuccess, ticketErr = pcall(function()
            return ReplicatedStorage:WaitForChild("API"):WaitForChild("PetRecyclerAPI/TicketsCollected"):InvokeServer()
        end)
        if ticketSuccess then
            log("üé´ Tickets collected successfully!")
            getTickets()
        end
        return true
    else
        log("‚ùå Failed to interact with PetRecycler")
        return false
    end
end

--------------------------------------------------------------
-- CLAIM REWARD FUNCTION (USING FOUND FOLDER)
--------------------------------------------------------------
local function claimReward()
    log("üîπ Claiming Reward...")
    
    -- INTERACT WITH THE FOUND RECYCLER DIRECTLY
    local claimSuccess = interactWithRecycler("claim")
    
    if claimSuccess then
        log("üéâ Successfully claimed reward using PetRecycler")
        return true
    else
        log("‚ùå Failed to claim reward")
        return false
    end
end

--------------------------------------------------------------
-- VERIFY RECYCLER ACCESS
--------------------------------------------------------------
local function verifyRecyclerAccess()
    petRecyclerModel = findPetRecyclerModel()
    if petRecyclerModel then
        log("‚úÖ PetRecycler found at: " .. petRecyclerModel:GetFullName())
        
        -- Check if it has ClickDetector
        local clickDetector = petRecyclerModel:FindFirstChild("ClickDetector")
        if clickDetector then
            log("‚úÖ Recycler has ClickDetector (interactive)")
        else
            log("‚ÑπÔ∏è Recycler may use remote events instead of ClickDetector")
        end
        
        -- Check parent folder
        local parentFolder = getRecyclerParentFolder()
        if parentFolder then
            log("‚úÖ Recycler is in folder: " .. parentFolder)
        else
            log("‚ÑπÔ∏è Recycler is in main map (will use direct interaction)")
        end
        
        return true
    else
        log("‚ö†Ô∏è PetRecycler not found in workspace - make sure you're near the event area")
        return false
    end
end

--------------------------------------------------------------
-- MAIN FARMING CYCLE
--------------------------------------------------------------
local function farmCycle()
    log("Starting farm cycle...")
    refreshInventory()
    getTickets()
    countCrystalEggs()
    antiAfk()

    -- Check conditions and get pet data
    local shouldRelease, releaseReason, allowedPets, neonPets, megaNeonPets = shouldReleaseNow()
    if not shouldRelease then
        log("‚ùå Not enough resources. Waiting " .. COOLDOWNS.RESOURCE_CHECK .. " seconds...")
        for i = 1, COOLDOWNS.RESOURCE_CHECK do
            if not running then break end
            if i % 30 == 0 then antiAfk() end
            task.wait(1)
        end
        return
    end

    log("‚úÖ Release conditions met! Reason: " .. releaseReason)

    -- PET RELEASE
    local releaseSuccess = executePetRelease(allowedPets, neonPets, megaNeonPets)

    if not releaseSuccess then
        log("‚ùå Release process failed")
        return
    end

    -- WAIT 7 MINUTES + 5 SECONDS COOLDOWN BEFORE CLAIMING
    log("‚è≥ Waiting " .. COOLDOWNS.REWARD_WAIT .. " seconds (" .. math.floor(COOLDOWNS.REWARD_WAIT/60) .. " minutes " .. (COOLDOWNS.REWARD_WAIT%60) .. " seconds) for rewards cooldown...")
    for i = 1, COOLDOWNS.REWARD_WAIT do
        if not running then break end
        if i % 60 == 0 then
            local minutesLeft = math.floor((COOLDOWNS.REWARD_WAIT - i) / 60)
            local secondsLeft = (COOLDOWNS.REWARD_WAIT - i) % 60
            log(" " .. minutesLeft .. "m " .. secondsLeft .. "s remaining...")
        end
        if i % 30 == 0 then antiAfk() end
        task.wait(1)
    end

    if not running then return end

    -- STEP 4: Claim Reward
    local claimSuccess = claimReward()
    if claimSuccess then
        log("‚úÖ Reward claimed successfully")
    else
        log("‚ö†Ô∏è Could not claim reward, will try next cycle")
    end

    -- Count crystal eggs after successful release
    refreshInventory()
    countCrystalEggs()
    getTickets()

    -- WAIT 30 SECONDS BEFORE NEXT CYCLE CHECK
    log("‚è≥ Waiting " .. COOLDOWNS.AFTER_CLAIM .. " seconds before next resource check...")
    for i = 1, COOLDOWNS.AFTER_CLAIM do
        if not running then break end
        if i % 30 == 0 then antiAfk() end
        task.wait(1)
    end

    return true
end

--------------------------------------------------------------
-- CONTROL SYSTEM
--------------------------------------------------------------
local function toggleFarm()
    running = not running
    if running then
        btn.Text = "‚èπÔ∏è"
        btn.TextColor3 = Color3.fromRGB(255, 0, 0)
        stroke.Color = Color3.fromRGB(255, 0, 0)
        status.Text = "RUNNING..."
        status.TextColor3 = Color3.fromRGB(0, 255, 0)
        log("Strict Filter Farmer started! Waiting for conditions...")
        
        -- Find and verify recycler on start
        verifyRecyclerAccess()
        
        farmThread = task.spawn(function()
            while running do
                local success, err = pcall(farmCycle)
                if not success then
                    log("Error in farm cycle: " .. tostring(err))
                end
                -- Only check every 60 seconds if no release happened
                if running then
                    log("‚è≥ Waiting " .. COOLDOWNS.RESOURCE_CHECK .. " seconds before next condition check...")
                    for i = 1, COOLDOWNS.RESOURCE_CHECK do
                        if not running then break end
                        if i % 30 == 0 then antiAfk() end
                        task.wait(1)
                    end
                end
            end
        end)
        pcall(function()
            TweenService:Create(btn, TweenInfo.new(0.6, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {
                Size = UDim2.new(0, 100, 0, 100)
            }):Play()
        end)
    else
        btn.Text = "‚ôªÔ∏è"
        btn.TextColor3 = Color3.fromRGB(0, 255, 0)
        stroke.Color = Color3.fromRGB(0, 255, 0)
        status.Text = "OFF"
        status.TextColor3 = Color3.fromRGB(255, 0, 0)
        if farmThread then task.cancel(farmThread) end
        pcall(function() btn.Size = UDim2.new(0, 90, 0, 90) end)
        log("Farmer stopped")
    end
end

btn.MouseButton1Click:Connect(function() pcall(toggleFarm) end)
player:GetMouse().KeyDown:Connect(function(key) if key:lower() == "r" then pcall(toggleFarm) end end)

-- Initial verification
task.wait(3)
verifyRecyclerAccess()

log("‚úÖ PROPER COOLDOWN Farmer Loaded! Press Recycle or R")
log("‚úÖ ALLOWED: 18 specific pets + ANY neon (age 6+) + 1 MEGA NEON (any age)")
log("‚úÖ USING FOUND RECYCLER: Direct interaction with PetRecycler model")
log("‚è∞ COOLDOWNS: 7m5s reward wait + 30s after claim + 60s resource checks")
