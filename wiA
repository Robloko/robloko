local function analyzeGingerbread(index)
    local gingerbread = workspace.Interiors["MainMap!Christmas"]:GetChildren()[index]
    if not gingerbread then return end
    
    print("=== Analyzing Gingerbread " .. index .. " ===")
    print("Name:", gingerbread.Name)
    print("ClassName:", gingerbread.ClassName)
    
    -- List all children
    for _, child in pairs(gingerbread:GetChildren()) do
        print("Child:", child.Name, "Type:", child.ClassName)
        
        -- Check ParticleHolder specifically
        if child.Name == "ParticleHolder" then
            for _, subChild in pairs(child:GetChildren()) do
                print("  Sub-child:", subChild.Name, "Type:", subChild.ClassName)
                if subChild:IsA("Attachment") then
                    for _, particle in pairs(subChild:GetChildren()) do
                        print("    Particle:", particle.Name, "Type:", particle.ClassName)
                    end
                end
            end
        end
    end
    print("=================================")
end

-- Use this to examine structure
analyzeGingerbread(82)
