local function debugCollectionMethods()
    print("=== DEBUG: Finding Collection Method ===")
    
    -- Get a gingerbread for testing
    local testGinger = workspace.Interiors["MainMap!Christmas"]:GetChildren()[82]
    if not testGinger then return end
    
    print("1. Checking RemoteStorage...")
    local rs = game:GetService("ReplicatedStorage")
    for _, child in pairs(rs:GetChildren()) do
        if child:IsA("RemoteEvent") or child:IsA("RemoteFunction") then
            print("  Found:", child.Name, "Type:", child.ClassName)
        end
    end
    
    print("\n2. Checking network folders...")
    local network = rs:FindFirstChild("adoptme_new_net") or rs:FindFirstChild("Network")
    if network then
        for _, remote in pairs(network:GetChildren()) do
            if remote:IsA("RemoteEvent") or remote:IsA("RemoteFunction") then
                print("  Network Remote:", remote.Name)
            end
        end
    end
    
    print("\n3. Checking gingerbread interaction...")
    local interactives = {
        ClickDetector = testGinger:FindFirstChildWhichIsA("ClickDetector"),
        ProximityPrompt = testGinger:FindFirstChildWhichIsA("ProximityPrompt"),
        TouchInterest = testGinger:FindFirstChildWhichIsA("TouchTransmitter")
    }
    
    for typeName, obj in pairs(interactives) do
        if obj then
            print("  Has", typeName, ":", obj.Name)
        end
    end
    
    print("=== DEBUG END ===")
end

-- Run this first to understand the collection method
debugCollectionMethods()
