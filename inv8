local function printAvailablePets()
    local success, errorMsg = pcall(function()
        local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
        local playerData = clientData.get_data()[player.Name]

        -- Выводим всю структуру playerData для анализа
        print("=== DEBUG: FULL PLAYER DATA STRUCTURE ===")
        for key, value in pairs(playerData) do
            print(key, ": ", value)
        end
        print("========================================")

        -- Проверяем возможные пути к данным о питомцах
        local petsData = playerData.pet_manager and playerData.pet_manager.pets
                      or playerData.pets
                      or (playerData.inventory and playerData.inventory.pets)
                      or {}

        -- Выводим структуру petsData
        print("=== DEBUG: PETS DATA STRUCTURE ===")
        for petId, petData in pairs(petsData) do
            print("Pet ID:", petId)
            print("Pet Data:", petData)
        end
        print("===================================")
    end)

    if not success then
        print("Error reading pets data: " .. tostring(errorMsg))
        return
    end
end

printAvailablePets()
