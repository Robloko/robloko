-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Player
local player = Players.LocalPlayer

-- Function to inspect EQUIP_MANAGER
local function inspectEquipManager()
    print("ğŸ” INSPECTING EQUIP_MANAGER")
    print("============================")
    
    local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
    local data = clientData.get_data()[player.Name]
    
    -- Check if EQUIP_MANAGER exists
    if data.EQUIP_MANAGER then
        print("âœ… EQUIP_MANAGER found!")
        print("Type: " .. type(data.EQUIP_MANAGER))
        
        -- Inspect the contents
        local equipManager = data.EQUIP_MANAGER
        
        if type(equipManager) == "table" then
            local itemCount = 0
            for _ in pairs(equipManager) do itemCount = itemCount + 1 end
            
            print("ğŸ“Š Total items in EQUIP_MANAGER: " .. itemCount)
            print("")
            
            -- Show all items
            for key, value in pairs(equipManager) do
                if type(value) == "table" then
                    print("ğŸ“ " .. key .. " [TABLE]")
                    -- Show table contents
                    for subKey, subValue in pairs(value) do
                        print("   " .. subKey .. " = " .. tostring(subValue))
                    end
                else
                    print("ğŸ“„ " .. key .. " = " .. tostring(value))
                end
                print("   ---")
            end
        else
            print("ğŸ“„ EQUIP_MANAGER value: " .. tostring(equipManager))
        end
    else
        print("âŒ EQUIP_MANAGER not found in player data")
        
        -- Show what categories are available to help find the correct name
        print("ğŸ” Available categories:")
        for categoryName, categoryData in pairs(data) do
            if string.find(string.upper(categoryName), "EQUIP") or 
               string.find(string.upper(categoryName), "MANAGER") then
                print("   ğŸ¯ " .. categoryName .. " (" .. type(categoryData) .. ")")
            end
        end
    end
end

-- Run the function
inspectEquipManager()
