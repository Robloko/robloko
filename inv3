-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Player
local player = Players.LocalPlayer

-- Function to inspect EQUIP_MANAGER
local function inspectEquipManager()
    print("üîç INSPECTING EQUIP_MANAGER")
    print("============================")
    
    local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
    local data = clientData.get_data()[player.Name]
    
    if not data.equip_manager then
        print("‚ùå equip_manager not found")
        return
    end
    
    print("üìä EQUIP_MANAGER categories:")
    
    -- Inspect each category in equip_manager
    for categoryName, categoryData in pairs(data.equip_manager) do
        print("")
        print("üéØ CATEGORY: " .. categoryName)
        print("   Type: " .. type(categoryData))
        
        if type(categoryData) == "table" then
            local itemCount = 0
            for _ in pairs(categoryData) do itemCount = itemCount + 1 end
            print("   Total items: " .. itemCount)
            
            -- Show all items in this category
            for itemId, itemData in pairs(categoryData) do
                print("   üì¶ Item ID: " .. tostring(itemId))
                
                if type(itemData) == "table" then
                    -- Show all fields in the item data
                    for fieldName, fieldValue in pairs(itemData) do
                        if type(fieldValue) == "table" then
                            print("      üìÅ " .. fieldName .. " [TABLE]")
                            -- Show first 3 sub-items
                            local subCount = 0
                            for subKey, subValue in pairs(fieldValue) do
                                subCount = subCount + 1
                                if subCount <= 3 then
                                    print("         " .. subKey .. " = " .. tostring(subValue))
                                end
                            end
                            if subCount > 3 then
                                print("         ... and " .. (subCount - 3) .. " more")
                            end
                        else
                            print("      üìÑ " .. fieldName .. " = " .. tostring(fieldValue))
                        end
                    end
                else
                    print("      Value: " .. tostring(itemData))
                end
                print("      ---")
            end
        else
            print("   Value: " .. tostring(categoryData))
        end
    end
end

-- Run the function
inspectEquipManager()
