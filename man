-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Player
local player = Players.LocalPlayer

-- Function to inspect specific categories from screenshots
local function inspectScreenshotCategories()
    print("üîç INSPECTING MANAGER CATEGORIES FROM SCREENSHOTS")
    print("=================================================")
    
    local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
    local data = clientData.get_data()[player.Name]
    
    -- Inspect AILMENTS_MANAGER
    if data.AILMENTS_MANAGER then
        print("üè• AILMENTS MANAGER:")
        for key, value in pairs(data.AILMENTS_MANAGER) do
            if type(value) == "table" then
                local count = 0
                for _ in pairs(value) do count = count + 1 end
                print("   üìÅ " .. key .. " [TABLE with " .. count .. " items]")
                
                -- Show first 3 items from each sub-table
                local subCount = 0
                for subKey, subValue in pairs(value) do
                    subCount = subCount + 1
                    if subCount <= 3 then
                        if type(subValue) == "table" then
                            local deepCount = 0
                            for _ in pairs(subValue) do deepCount = deepCount + 1 end
                            print("     üìÅ " .. subKey .. " [TABLE with " .. deepCount .. " items]")
                        else
                            print("     üìÑ " .. subKey .. " = " .. tostring(subValue))
                        end
                    end
                end
                if subCount > 3 then
                    print("     ... and " .. (subCount - 3) .. " more items")
                end
            else
                print("   üìÑ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    else
        print("‚ùå AILMENTS_MANAGER not found\n")
    end
    
    -- Inspect PET_STATE_MANAGERS
    if data.PET_STATE_MANAGERS then
        print("üêæ PET STATE MANAGERS:")
        for key, value in pairs(data.PET_STATE_MANAGERS) do
            if type(value) == "table" then
                local count = 0
                for _ in pairs(value) do count = count + 1 end
                print("   üìÅ " .. key .. " [TABLE with " .. count .. " items]")
                
                -- Show first 3 items from each sub-table
                local subCount = 0
                for subKey, subValue in pairs(value) do
                    subCount = subCount + 1
                    if subCount <= 3 then
                        if type(subValue) == "table" then
                            local deepCount = 0
                            for _ in pairs(subValue) do deepCount = deepCount + 1 end
                            print("     üìÅ " .. subKey .. " [TABLE with " .. deepCount .. " items]")
                        else
                            print("     üìÑ " .. subKey .. " = " .. tostring(subValue))
                        end
                    end
                end
                if subCount > 3 then
                    print("     ... and " .. (subCount - 3) .. " more items")
                end
            else
                print("   üìÑ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    else
        print("‚ùå PET_STATE_MANAGERS not found\n")
    end
    
    -- Inspect QUEST_MANAGER
    if data.QUEST_MANAGER then
        print("üéØ QUEST MANAGER:")
        for key, value in pairs(data.QUEST_MANAGER) do
            if type(value) == "table" then
                local count = 0
                for _ in pairs(value) do count = count + 1 end
                print("   üìÅ " .. key .. " [TABLE with " .. count .. " items]")
                
                -- Show first 3 items from each sub-table
                local subCount = 0
                for subKey, subValue in pairs(value) do
                    subCount = subCount + 1
                    if subCount <= 3 then
                        if type(subValue) == "table" then
                            local deepCount = 0
                            for _ in pairs(subValue) do deepCount = deepCount + 1 end
                            print("     üìÅ " .. subKey .. " [TABLE with " .. deepCount .. " items]")
                        else
                            print("     üìÑ " .. subKey .. " = " .. tostring(subValue))
                        end
                    end
                end
                if subCount > 3 then
                    print("     ... and " .. (subCount - 3) .. " more items")
                end
            else
                print("   üìÑ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    else
        print("‚ùå QUEST_MANAGER not found\n")
    end
    
    -- Inspect ADMIN_ABUSE_MANAGER
    if data.ADMIN_ABUSE_MANAGER then
        print("‚ö° ADMIN ABUSE MANAGER:")
        for key, value in pairs(data.ADMIN_ABUSE_MANAGER) do
            if type(value) == "table" then
                local count = 0
                for _ in pairs(value) do count = count + 1 end
                print("   üìÅ " .. key .. " [TABLE with " .. count .. " items]")
                
                -- Show first 3 items from each sub-table
                local subCount = 0
                for subKey, subValue in pairs(value) do
                    subCount = subCount + 1
                    if subCount <= 3 then
                        if type(subValue) == "table" then
                            local deepCount = 0
                            for _ in pairs(subValue) do deepCount = deepCount + 1 end
                            print("     üìÅ " .. subKey .. " [TABLE with " .. deepCount .. " items]")
                        else
                            print("     üìÑ " .. subKey .. " = " .. tostring(subValue))
                        end
                    end
                end
                if subCount > 3 then
                    print("     ... and " .. (subCount - 3) .. " more items")
                end
            else
                print("   üìÑ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    else
        print("‚ùå ADMIN_ABUSE_MANAGER not found\n")
    end
    
    -- Inspect STATE_MANAGER_RAW
    if data.STATE_MANAGER_RAW then
        print("üîß STATE MANAGER RAW:")
        for key, value in pairs(data.STATE_MANAGER_RAW) do
            if type(value) == "table" then
                local count = 0
                for _ in pairs(value) do count = count + 1 end
                print("   üìÅ " .. key .. " [TABLE with " .. count .. " items]")
                
                -- Show first 3 items from each sub-table
                local subCount = 0
                for subKey, subValue in pairs(value) do
                    subCount = subCount + 1
                    if subCount <= 3 then
                        if type(subValue) == "table" then
                            local deepCount = 0
                            for _ in pairs(subValue) do deepCount = deepCount + 1 end
                            print("     üìÅ " .. subKey .. " [TABLE with " .. deepCount .. " items]")
                        else
                            print("     üìÑ " .. subKey .. " = " .. tostring(subValue))
                        end
                    end
                end
                if subCount > 3 then
                    print("     ... and " .. (subCount - 3) .. " more items")
                end
            else
                print("   üìÑ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    else
        print("‚ùå STATE_MANAGER_RAW not found\n")
    end
end

-- Run the function
inspectScreenshotCategories()
