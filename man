-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Player
local player = Players.LocalPlayer

-- Function to inspect specific categories from screenshots
local function inspectScreenshotCategories()
    print("ğŸ” INSPECTING CATEGORIES FROM SCREENSHOTS")
    print("=========================================")
    
    local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
    local data = clientData.get_data()[player.Name]
    
    -- Inspect ailments_manager
    if data.ailments_manager then
        print("ğŸ¥ AILMENTS MANAGER:")
        for key, value in pairs(data.ailments_manager) do
            if type(value) == "table" then
                print("   ğŸ“ " .. key .. ":")
                for subKey, subValue in pairs(value) do
                    if type(subValue) == "table" then
                        print("     ğŸ“ " .. subKey .. ":")
                        for deepKey, deepValue in pairs(subValue) do
                            print("       " .. deepKey .. " = " .. tostring(deepValue))
                        end
                    else
                        print("     " .. subKey .. " = " .. tostring(subValue))
                    end
                end
            else
                print("   ğŸ“„ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    end
    
    -- Inspect pet_state_managers
    if data.pet_state_managers then
        print("ğŸ¾ PET STATE MANAGERS:")
        for key, value in pairs(data.pet_state_managers) do
            if type(value) == "table" then
                print("   ğŸ“ " .. key .. ":")
                for subKey, subValue in pairs(value) do
                    if type(subValue) == "table" then
                        print("     ğŸ“ " .. subKey .. ":")
                        for deepKey, deepValue in pairs(subValue) do
                            print("       " .. deepKey .. " = " .. tostring(deepValue))
                        end
                    else
                        print("     " .. subKey .. " = " .. tostring(subValue))
                    end
                end
            else
                print("   ğŸ“„ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    end
    
    -- Inspect quest_manager
    if data.quest_manager then
        print("ğŸ¯ QUEST MANAGER:")
        for key, value in pairs(data.quest_manager) do
            if type(value) == "table" then
                print("   ğŸ“ " .. key .. ":")
                for subKey, subValue in pairs(value) do
                    if type(subValue) == "table" then
                        print("     ğŸ“ " .. subKey .. ":")
                        for deepKey, deepValue in pairs(subValue) do
                            print("       " .. deepKey .. " = " .. tostring(deepValue))
                        end
                    else
                        print("     " .. subKey .. " = " .. tostring(subValue))
                    end
                end
            else
                print("   ğŸ“„ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    end
    
    -- Inspect admin_abuse_manager
    if data.admin_abuse_manager then
        print("âš¡ ADMIN ABUSE MANAGER:")
        for key, value in pairs(data.admin_abuse_manager) do
            if type(value) == "table" then
                print("   ğŸ“ " .. key .. ":")
                for subKey, subValue in pairs(value) do
                    if type(subValue) == "table" then
                        print("     ğŸ“ " .. subKey .. ":")
                        for deepKey, deepValue in pairs(subValue) do
                            print("       " .. deepKey .. " = " .. tostring(deepValue))
                        end
                    else
                        print("     " .. subKey .. " = " .. tostring(subValue))
                    end
                end
            else
                print("   ğŸ“„ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    end
    
    -- Inspect state_manager_raw
    if data.state_manager_raw then
        print("ğŸ”§ STATE MANAGER RAW:")
        for key, value in pairs(data.state_manager_raw) do
            if type(value) == "table" then
                print("   ğŸ“ " .. key .. ":")
                for subKey, subValue in pairs(value) do
                    if type(subValue) == "table" then
                        print("     ğŸ“ " .. subKey .. ":")
                        for deepKey, deepValue in pairs(subValue) do
                            print("       " .. deepKey .. " = " .. tostring(deepValue))
                        end
                    else
                        print("     " .. subKey .. " = " .. tostring(subValue))
                    end
                end
            else
                print("   ğŸ“„ " .. key .. " = " .. tostring(value))
            end
        end
        print("")
    end
end

-- Function to list all available manager categories
local function listAllManagers()
    print("ğŸ“‹ LISTING ALL MANAGER CATEGORIES")
    print("=================================")
    
    local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
    local data = clientData.get_data()[player.Name]
    
    local managerCount = 0
    
    for categoryName, categoryData in pairs(data) do
        if string.find(string.lower(categoryName), "manager") then
            managerCount = managerCount + 1
            local itemCount = 0
            if type(categoryData) == "table" then
                for _ in pairs(categoryData) do itemCount = itemCount + 1 end
            end
            print("ğŸ¯ " .. categoryName .. " (" .. itemCount .. " items)")
        end
    end
    
    if managerCount == 0 then
        print("âŒ No manager categories found")
    else
        print("\nğŸ“Š Total manager categories: " .. managerCount)
    end
    print("")
end

-- Run the functions
listAllManagers()
inspectScreenshotCategories()
