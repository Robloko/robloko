local function bruteForceAPIs()
    print("=== BRUTE FORCE ALL APIs ===")
    
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local apiFolder = ReplicatedStorage.API
    
    local testValues = {"hungry", "thirsty", "sleepy", "dirty", "Feed", "Drink", "Sleep", "Clean", 1, true}
    
    for _, api in ipairs(apiFolder:GetChildren()) do
        print("üéØ NEXT API TO TEST: " .. api.Name)
        print("‚è∞ Waiting 5 seconds...")
        
        for i = 1, 5 do
            print(i .. "...")
            task.wait(1)
        end
        
        print("üöÄ TESTING API: " .. api.Name)
        
        -- Try different parameter combinations
        for i = 1, 3 do  -- Try up to 3 parameters
            for _, v1 in ipairs(testValues) do
                local success, err = pcall(function()
                    if i == 1 then
                        api:FireServer(v1)
                    elseif i == 2 then
                        api:FireServer(v1, 1)
                    else
                        api:FireServer(v1, 1, true)
                    end
                end)
                
                if success then
                    print("‚úÖ WORKING: " .. api.Name .. " with " .. i .. " parameters")
                    print("   First param: " .. tostring(v1))
                end
            end
        end
        
        print("---")
    end
    
    print("=== BRUTE FORCE COMPLETE ===")
end

bruteForceAPIs()
