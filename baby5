local function debugSimple()
    print("=== SIMPLE DEBUG ===")
    
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local Players = game:GetService("Players")
    local localPlayer = Players.LocalPlayer
    
    -- Get basic player data
    local clientData = require(ReplicatedStorage.ClientModules.Core.ClientData)
    local playerData = clientData.get_data()[localPlayer.Name]
    
    -- Just show the main keys
    print("Player data keys:")
    for key, value in pairs(playerData) do
        print("- " .. key)
    end
    
    -- Check if we have current_baby or similar
    if playerData.current_baby then
        print("✅ current_baby:", playerData.current_baby)
    end
    
    if playerData.selected_baby then
        print("✅ selected_baby:", playerData.selected_baby)
    end
    
    if playerData.baby then
        print("✅ baby data exists")
    end
    
    print("=== DEBUG COMPLETE ===")
end

debugSimple()
